<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/results_publish_answer_sheet_examName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="2dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="12sp"
        android:text="dsfjhkdskfdsdfk" 
        android:textColor="#000000" />

    <TextView
        android:id="@+id/results_publish_answer_sheet_section"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:textSize="12sp"
        android:layout_marginLeft="5dp"
        android:text="" />

    <LinearLayout
        android:id="@+id/results_publish_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="2dp"
        android:visibility="visible" >

        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_questions_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total Questions"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>

            <TextView
                android:id="@+id/results_publish_answer_sheet_Marks_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/results_publish_answer_sheet_questions_view"
                android:text="Total Marks"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/totalMarksAwarded_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total Awarded Marks"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="10dp"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_questions_colon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/results_publish_answer_sheet_Marks_colon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/totalMarksAwarded_colon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="30dp"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_questions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>

            <TextView
                android:id="@+id/results_publish_answer_sheet_Marks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>

            <TextView
                android:id="@+id/totalMarksAwarded"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="55dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_startDate_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Start Date"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/results_publish_answer_sheet_endDate_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="End Date"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/results_publish_answer_sheet_time_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Duration"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="10dp"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_time_semi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>

            <TextView
                android:id="@+id/results_publish_answer_sheet_startDate_semi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />

            <TextView
                android:id="@+id/results_publish_answer_sheet_endDate_semi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="180dp"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/results_publish_answer_sheet_startDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>
            
            <TextView
                android:id="@+id/results_publish_answer_sheet_endDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall" 
                 android:textSize="12sp"/>

            <TextView
                android:id="@+id/results_publish_answer_sheet_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textAppearance="?android:attr/textAppearanceSmall"
                 android:textSize="12sp" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/results_publish_answerSheetLayout"
        android:layout_width="match_parent"
        android:layout_height="220dp"
        android:orientation="vertical" >
    </LinearLayout>

</LinearLayout>